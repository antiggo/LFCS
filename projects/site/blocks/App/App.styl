
@font-face {
    font-family: 'GT Walsheim Pro';
    src: url('/assets/fonts/GTWalshteimPro/GTWalsheimPro-Regular.eot');
    src: local('GT Walsheim Pro Regular'), local('GTWalsheimPro-Regular'),
        url('/assets/fonts/GTWalshteimPro/GTWalsheimPro-Regular.eot?#iefix') format('embedded-opentype'),
        url('/assets/fonts/GTWalshteimPro/GTWalsheimPro-Regular.woff2') format('woff2'),
        url('/assets/fonts/GTWalshteimPro/GTWalsheimPro-Regular.woff') format('woff'),
        url('/assets/fonts/GTWalshteimPro/GTWalsheimPro-Regular.ttf') format('truetype');
    font-weight: normal;
    font-style: normal;
}

@font-face {
    font-family: 'GT Walsheim Pro';
    src: url('/assets/fonts/GTWalshteimPro/GTWalsheimPro-Thin.eot');
    src: local('GT Walsheim Pro Thin'), local('GTWalsheimPro-Thin'),
        url('/assets/fonts/GTWalshteimPro/GTWalsheimPro-Thin.eot?#iefix') format('embedded-opentype'),
        url('/assets/fonts/GTWalshteimPro/GTWalsheimPro-Thin.woff2') format('woff2'),
        url('/assets/fonts/GTWalshteimPro/GTWalsheimPro-Thin.woff') format('woff'),
        url('/assets/fonts/GTWalshteimPro/GTWalsheimPro-Thin.ttf') format('truetype');
    font-weight: 100;
    font-style: normal;
}

.Screen
    height 100%

.List
    margin 66px 20px
    &__hint
        margin-top 10px
        color #ACACAC
    &__item
        
        display block
        max-width 70%
        @media screen-phone
            max-width 100%
        & + &
            margin-top 100px
    a
        text-decoration none
        color inherit
    &__title
        color #C49B72

table
    border-collapse collapse
    border-spacing 0
    border none

td
    padding 0
    vertical-align baseline

html
    background var(--ground)
    

.App
    background #111111
    color white
    padding 0
    margin 0
    font-family GT Walsheim Pro
    font-weight normal
    font-smoothing antialiased
    -webkit-font-smoothing antialiased
    -moz-osx-font-smoothing grayscale
    
:root
    --ground #CAC5C4
    --text #2C2B2B
    --formHighlight #000
    --formPlaceholder #999 
    
.Title
    margin 70px 20px 20px 20px
    // font-size 76px
    // line-height 52px
    // letter-spacing -5px

.Text
    color inherit
    &_Gap
        &_Main
            margin 200px 20px 68px 20px
            max-width 78%
            @media screen-phone
                margin 120px 20px 38px 20px
                max-width initial
        &_M
            margin 34px 20px 42px 20px
            max-width 70%
            @media screen-phone
                // margin 120px 20px 38px 20px
                max-width initial
    
